{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true
  },
  "biome.enabled": true,
  "biome.lsp.bin": {
    "win32-x64": "./web/node_modules/@biomejs/cli-win32-x64/biome.exe",
    "linux-x64": "./web/node_modules/@biomejs/cli-linux-x64/biome",
    "darwin-x64": "./web/node_modules/@biomejs/cli-darwin-x64/biome",
  }
}